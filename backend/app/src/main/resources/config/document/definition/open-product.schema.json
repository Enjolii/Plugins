{
  "$id": "open-product.schema",
  "type": "object",
  "title": "Open Product",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "result": {
      "type": "string"
    },
    "status": {
      "enum": [
        "initieel",
        "gereed",
        "actief",
        "ingetrokken",
        "geweigerd",
        "verlopen"
      ],
      "type": "string"
    },
    "ownerBSN": {
      "type": "string"
    },
    "published": {
      "type": "boolean"
    },
    "frequentie": {
      "enum": [
        "eenmalig",
        "jaarlijks",
        "maandelijks"
      ],
      "type": "string"
    },
    "productName": {
      "type": "string"
    },
    "productUUID": {
      "type": "string"
    },
    "productPrice": {
      "type": "string"
    },
    "productTypeUUID": {
      "type": "string"
    }
  },
  "additionalProperties": false
}